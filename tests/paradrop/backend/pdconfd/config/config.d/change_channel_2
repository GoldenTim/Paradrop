config wifi-device wifi0
    option type 'auto'
    option channel '6'

config wifi-iface
    option device 'wifi0'
    option mode 'ap'
    option ssid 'Paradrop'
    option network 'lan'

config interface lan
    option proto 'dhcp'
    option type 'bridge'
    list ifname 'eth1'
    list ifname 'wlan0'
